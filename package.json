{
  "name": "nestjs-telegraf",
  "version": "2.8.1",
  "description": "Telegraf module for NestJS",
  "keywords": [
    "nest",
    "nestjs",
    "nestjs-module",
    "telegraf",
    "telegram",
    "telegram bot",
    "telegram bot api",
    "bot",
    "bot api",
    "bot framework"
  ],
  "homepage": "https://nestjs-telegraf.0x467.com/",
  "bugs": {
    "url": "https://github.com/0x467/nestjs-telegraf/issues"
  },
  "repository": "git@github.com:0x467/nestjs-telegraf.git",
  "license": "MIT",
  "author": "0x467 <0x467@pm.me>",
  "contributors": [
    {
      "name": "Arthur Asimov",
      "email": "arthur.asimov.z0@gmail.com"
    }
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "rm -rf dist && tsc -p tsconfig.json",
    "precommit": "lint-staged",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public",
    "test": "",
    "typedoc:build": "typedoc --tsconfig ./tsconfig.typedoc.json",
    "docs:dev": "vitepress dev",
    "docs:build": "vitepress build",
    "docs:preview": "vitepress preview"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -c .commitlintrc.json -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@commitlint/cli": "19.3.0",
    "@commitlint/config-angular": "19.3.0",
    "@nestjs/common": "^10.3.9",
    "@nestjs/core": "^10.3.9",
    "@types/lodash": "4.17.5",
    "@typescript-eslint/eslint-plugin": "7.13.1",
    "@typescript-eslint/parser": "7.13.1",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "husky": "9.0.11",
    "lint-staged": "15.2.7",
    "prettier": "3.3.2",
    "reflect-metadata": "^0.2.2",
    "telegraf": "^4.16.3",
    "typedoc": "0.26.2",
    "typescript": "5.5.2",
    "vitepress": "^1.2.3"
  },
  "peerDependencies": {
    "@nestjs/common": "^9.0.0 || ^10.0.0",
    "@nestjs/core": "^9.0.0 || ^10.0.0",
    "reflect-metadata": "^0.2.2",
    "telegraf": "^4.0.0",
    "typescript": "^4.1.2 || ^5.0.0"
  }
}
