{
  "name": "nestjs-telegraf",
  "version": "2.7.0",
  "description": "Telegraf module for NestJS",
  "keywords": [
    "nest",
    "nestjs",
    "nestjs-module",
    "telegraf",
    "telegram",
    "telegram bot",
    "telegram bot api",
    "bot",
    "bot api",
    "bot framework"
  ],
  "homepage": "https://nestjs-telegraf.vercel.app",
  "bugs": {
    "url": "https://github.com/evilsprut/nestjs-telegraf/issues"
  },
  "repository": "git@github.com:evilsprut/nestjs-telegraf.git",
  "funding": "https://hypeer.company/donate",
  "license": "MIT",
  "author": "Aleksandr Bukhalo <evilsprut@icloud.com>",
  "contributors": [
    {
      "name": "Arthur Asimov",
      "email": "arthur.asimov.z0@gmail.com"
    }
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "rm -rf dist && tsc -p tsconfig.json",
    "precommit": "lint-staged",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public",
    "test": "",
    "typedoc:build": "typedoc --tsconfig ./tsconfig.typedoc.json",
    "docs:dev": "vitepress dev",
    "docs:build": "vitepress build",
    "docs:preview": "vitepress preview"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -c .commitlintrc.json -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@commitlint/cli": "17.4.4",
    "@commitlint/config-angular": "17.1.0",
    "@nestjs/common": "^9.0.11",
    "@nestjs/core": "^9.0.11",
    "@types/lodash": "4.14.191",
    "@typescript-eslint/eslint-plugin": "5.54.0",
    "@typescript-eslint/parser": "5.45.0",
    "eslint": "8.45.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "8.0.1",
    "lint-staged": "13.2.3",
    "prettier": "3.0.0",
    "reflect-metadata": "0.1.13",
    "telegraf": "4.12.2",
    "typedoc": "0.24.8",
    "typescript": "4.8.2",
    "vitepress": "^1.0.0-beta.6"
  },
  "peerDependencies": {
    "@nestjs/common": "^9.0.0 || ^10.0.0",
    "@nestjs/core": "^9.0.0 || ^10.0.0",
    "reflect-metadata": "^0.1.13",
    "telegraf": "^4.0.0",
    "typescript": "^4.1.2 || ^5.0.0"
  }
}
